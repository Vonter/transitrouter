{
  "name": "busrouter-sg",
  "version": "2.1.0",
  "description": "Singapore Bus Routes Explorer",
  "author": "Lim Chee Aun",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve index.html bus-arrival/index.html bus-first-last/index.html visualization/index.html -p 8888",
    "build": "parcel build index.html bus-arrival/index.html bus-first-last/index.html visualization/index.html --no-cache",
    "postbuild": "cpy {favicon.ico,ads.txt} dist && cpy well-known/* dist/.well-known && node tasks/generateHeaders",
    "prod": "npx serve dist/"
  },
  "dependencies": {
    "@deck.gl/core": "8.6.5",
    "@deck.gl/layers": "8.6.5",
    "@deck.gl/mapbox": "8.6.5",
    "@mapbox/mapbox-gl-language": "~1.0.0",
    "@sentry/react": "~6.16.1",
    "@sentry/tracing": "~6.16.1",
    "cheap-ruler": "~3.0.1",
    "date-fns": "~2.28.0",
    "fuse.js": "~6.5.3",
    "i18next": "~21.6.6",
    "i18next-browser-languagedetector": "~6.1.2",
    "just-intersect": "~4.0.2",
    "lscache": "~1.3.0",
    "preact": "~10.6.4",
    "react-i18next": "~11.15.3",
    "use-resize-observer": "~8.0.0",
    "workbox-cacheable-response": "~6.4.2",
    "workbox-expiration": "~6.4.2",
    "workbox-google-analytics": "~6.4.2",
    "workbox-routing": "~6.4.2",
    "workbox-strategies": "~6.4.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "~7.16.7",
    "@mapbox/polyline": "~1.1.1",
    "@parcel/babel-plugin-transform-runtime": "~2.2.0",
    "@parcel/babel-preset-env": "~2.2.0",
    "@parcel/config-default": "~2.2.0",
    "@parcel/packager-raw-url": "~2.2.0",
    "@parcel/transformer-image": "~2.2.0",
    "@parcel/transformer-webmanifest": "~2.2.0",
    "@turf/circle": "~6.5.0",
    "@turf/helpers": "~6.5.0",
    "@turf/simplify": "~6.5.0",
    "autoprefixer": "~10.4.2",
    "babel-plugin-transform-remove-console": "~6.9.4",
    "cpy-cli": "~3.1.1",
    "got": "~12.0.1",
    "parcel": "~2.2.0"
  },
  "alias": {
    "react": "preact/compat"
  },
  "engines": {
    "node": ">=14.10"
  },
  "browserslist": [
    ">0.25%",
    "not dead",
    "not ie 11",
    "not chrome < 51",
    "not op_mini all",
    "not Android > 1",
    "not safari < 10"
  ]
}
